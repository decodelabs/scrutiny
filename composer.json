{
    "name": "decodelabs/scrutiny",
    "description": "Unified human check controls",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.8",
        "decodelabs/coercion": "^0.3",
        "decodelabs/compass": "^0.3",
        "decodelabs/dictum": "^0.6.9",
        "decodelabs/exceptional": "^0.5.3",
        "decodelabs/glitch-support": "^0.5.1",
        "decodelabs/hydro": "^0.1.6",
        "decodelabs/slingshot": "^0.1.12",
        "decodelabs/tagged": "^0.16",
        "decodelabs/veneer": "^0.12.3",

        "symfony/polyfill-php82": "^1.31"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/dovetail": "^0.2.9"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Scrutiny\\": "src/Scrutiny"
        },
        "classmap": [
            "src/Dovetail/"
        ],
        "files": [
            "src/Scrutiny/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
