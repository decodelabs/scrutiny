{
    "name": "decodelabs/scrutiny",
    "description": "Unified human check controls",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.4",
        "decodelabs/coercion": "^0.3.5",
        "decodelabs/compass": "^0.4",
        "decodelabs/dictum": "^0.7",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/horizon": "^0.5",
        "decodelabs/hydro": "^0.2",
        "decodelabs/kingdom": "^0.1",
        "decodelabs/monarch": "^0.2.1",
        "decodelabs/nuance": "^0.1.9",
        "decodelabs/slingshot": "^0.3",
        "decodelabs/tagged": "^0.21"
    },
    "require-dev": {
        "decodelabs/dovetail": "^0.3.1",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/dovetail": "<0.3"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Scrutiny\\": "src/Scrutiny"
        },
        "classmap": [
            "src/Dovetail/",
            "src/Tagged/",
            "src/Scrutiny.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
