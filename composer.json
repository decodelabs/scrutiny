{
    "name": "decodelabs/scrutiny",
    "description": "Unified human check controls",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/compass": "^0.3.1",
        "decodelabs/dictum": "^0.6.10",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/horizon": "^0.4.8",
        "decodelabs/hydro": "^0.1.9",
        "decodelabs/nuance": "^0.1.2",
        "decodelabs/slingshot": "^0.2.1",
        "decodelabs/tagged": "^0.20.1",
        "decodelabs/veneer": "^0.12.8"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/dovetail": "^0.2.14"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Scrutiny\\": "src/Scrutiny"
        },
        "classmap": [
            "src/Dovetail/",
            "src/Tagged/"
        ],
        "files": [
            "src/Scrutiny/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
